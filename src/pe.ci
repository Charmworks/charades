module pe {

  group PE {
    entry PE(); /**< default constructor */
    entry PE(int, int); /**< constructor */
    entry void GVT_begin(); /**< begin gvt computation */
    entry void GVT_contribute(); /**< reduction target for completion of GVT */
    entry void GVT_end(); /**< reduction target for completion of GVT */
    entry void execute(); /**< move one step further, till the end of simulation */
  };
}
