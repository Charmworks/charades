# Small test with 16 lps, 4 per chare and load balancing enabled.
./charmrun +p2 ./phold --num-lps=16 --lps-per-chare=4 --synch=2 +balancer RotateLB
./charmrun +p4 ./phold --num-lps=16 --lps-per-chare=4 --synch=2 +balancer RotateLB
./charmrun +p2 ./phold --num-lps=16 --lps-per-chare=4 --synch=3 +balancer RotateLB
./charmrun +p4 ./phold --num-lps=16 --lps-per-chare=4 --synch=3 +balancer RotateLB
